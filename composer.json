{
  "type": "project",
  "name": "t3o/gettypo3org",
  "license": "GPL-v2+",
  "authors": [
    {
      "name": "Susanne Moog",
      "email": "susanne.moog@typo3.com"
    }, {
      "name": "Benni Mack",
      "email": "benni@typo3.org"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "ext-iconv": "*",
    "symfony/console": "^4.0",
    "symfony/flex": "^1.0",
    "symfony/framework-bundle": "^4.0",
    "symfony/lts": "^4@dev",
    "symfony/twig-bundle": "^4.0",
    "symfony/yaml": "^4.0"
  },
  "require-dev": {
    "symfony/dotenv": "^4.0",
    "symfony/profiler-pack": "^1.0"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "T3O\\GetTypo3Org\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "T3O\\GetTypo3Org\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "id": "01C8T4G8QYJT0T9Q9SWDHVW8AC",
      "allow-contrib": false
    }
  }
}
